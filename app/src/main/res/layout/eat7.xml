<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/eat_7_question"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="최근 보건소, 구청, 주민센터, 복지시설, 학교, 병원 등에서 실시된 영양 교육 및 상담을 받은 적이 있습니까?"/>
        <RadioButton
            android:layout_below="@id/eat_7_question"
            android:id="@+id/eat_71"
            android:text="예"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <RadioButton
            android:layout_below="@id/eat_71"
            android:id="@+id/eat_72"
            android:text="아니오"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <RadioButton
            android:layout_below="@id/eat_72"
            android:id="@+id/eat_73"
            android:text="비해당"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>