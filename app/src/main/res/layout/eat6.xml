<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/eat_6_question"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="최근 2주이상 지속적으로 식이보충제를 복용한 적이 있습니까?"/>
        <RadioGroup
            android:layout_below="@id/eat_6_question"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <RadioButton
                android:id="@+id/eat_61"
                android:text="예"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <RadioButton
                android:layout_below="@id/eat_61"
                android:id="@+id/eat_62"
                android:text="아니오"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RadioGroup>



    </RelativeLayout>


</android.support.constraint.ConstraintLayout>